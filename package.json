{
  "name": "tsheets-sdk",
  "version": "1.0.0-0",
  "description": "TSheets JS SDK for full API access.",
  "main": "./lib/index.js",
  "module": "./es/tsheets-sdk.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "yarn lint -- --fix",
    "clean": "rimraf lib coverage es",
    "test": "nf run -- mocha --compilers js:babel-core/register ./test/setup.js",
    "test:all": "yarn test ./test/**/*.spec.js",
    "test:cov": "babel-node $(npm bin)/isparta cover --report lcov $(npm bin)/_mocha -- ./test --recursive",
    "build:lib": "babel src --out-dir lib",
    "build": "yarn clean && yarn build:lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "watch": "yarn build:lib -- --watch",
    "prepublish": "yarn clean && yarn lint:fix && yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:prescottprue/tsheets-sdk.git"
  },
  "keywords": [
    "tsheets",
    "time",
    "tracking"
  ],
  "author": "prescottprue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prescottprue/tsheets-sdk/issues"
  },
  "homepage": "https://github.com/prescottprue/tsheets-sdk",
  "devDependencies": {
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6",
    "babel-preset-env": "^1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "codecov": "^1.0.1",
    "cross-env": "^5.1.4",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "foreman": "^2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "rimraf": "^2.5.1"
  },
  "dependencies": {
    "request": "^2.72.0"
  }
}
